                /* Live Features Showcase Styles */
                .live-features-showcase {
                    background: var(--bg-primary);
                    position: relative;
                    overflow: hidden;
                }

                .feature-demo {
                    min-height: 100vh;
                    padding: 6rem 0;
                    position: relative;
                    border-bottom: 1px solid rgba(255, 255, 255, 0.1);
                }

                .feature-title-section {
                    text-align: center;
                    margin-bottom: 4rem;
                    padding: 0 2rem;
                }

                .mega-feature-title {
                    font-size: clamp(3rem, 8vw, 6rem);
                    font-weight: var(--font-bold);
                    background: var(--gradient-hero);
                    -webkit-background-clip: text;
                    -webkit-text-fill-color: transparent;
                    background-clip: text;
                    margin-bottom: 1rem;
                    line-height: 1.1;
                }

                .demo-container {
                    max-width: 1200px;
                    margin: 0 auto;
                    padding: 0 2rem;
                }

                /* Hero Video Demo Styles */
                .video-hero-demo {
                    position: relative;
                    height: 500px;
                    border-radius: var(--rounded-2xl);
                    overflow: hidden;
                    box-shadow: var(--shadow-2xl);
                }

                .video-background {
                    position: absolute;
                    top: 0;
                    left: 0;
                    width: 100%;
                    height: 100%;
                }

                .animated-bg {
                    width: 100%;
                    height: 100%;
                    background: linear-gradient(45deg, #667eea, #764ba2, #f093fb, #f5576c);
                    background-size: 400% 400%;
                    animation: gradientShift 8s ease infinite;
                }

                @keyframes gradientShift {
                    0% { background-position: 0% 50%; }
                    50% { background-position: 100% 50%; }
                    100% { background-position: 0% 50%; }
                }

                .video-overlay {
                    position: absolute;
                    top: 0;
                    left: 0;
                    width: 100%;
                    height: 100%;
                    background: rgba(0, 0, 0, 0.4);
                }

                .hero-content-demo {
                    position: absolute;
                    top: 50%;
                    left: 50%;
                    transform: translate(-50%, -50%);
                    text-align: center;
                    z-index: 2;
                    color: white;
                }

                .demo-title {
                    font-size: 3rem;
                    margin-bottom: 1rem;
                    font-weight: var(--font-bold);
                }

                .demo-subtitle {
                    font-size: 1.2rem;
                    margin-bottom: 2rem;
                    opacity: 0.9;
                }

                .demo-btn {
                    padding: 1rem 2rem;
                    background: var(--gradient-primary);
                    border: none;
                    border-radius: var(--rounded-lg);
                    color: white;
                    font-weight: var(--font-semibold);
                    cursor: pointer;
                    transition: transform 0.2s ease;
                }

                .demo-btn:hover {
                    transform: translateY(-2px);
                }

                .video-controls {
                    position: absolute;
                    bottom: 20px;
                    right: 20px;
                    display: flex;
                    gap: 10px;
                    z-index: 3;
                }

                .control-btn {
                    padding: 0.5rem 1rem;
                    background: rgba(255, 255, 255, 0.2);
                    border: 1px solid rgba(255, 255, 255, 0.3);
                    border-radius: var(--rounded-md);
                    color: white;
                    cursor: pointer;
                    transition: all 0.2s ease;
                }

                .control-btn.active {
                    background: rgba(255, 255, 255, 0.3);
                }

                /* Carousel Demo Styles */
                .carousel-demo-wrapper {
                    text-align: center;
                }

                .carousel-container {
                    position: relative;
                    max-width: 800px;
                    height: 400px;
                    margin: 0 auto 2rem;
                    border-radius: var(--rounded-2xl);
                    overflow: hidden;
                    box-shadow: var(--shadow-2xl);
                }

                .carousel-track {
                    display: flex;
                    width: 100%;
                    height: 100%;
                    transition: transform 0.5s ease;
                }

                .carousel-slide {
                    min-width: 100%;
                    height: 100%;
                    display: flex;
                    align-items: center;
                    justify-content: center;
                    position: relative;
                }

                .slide-content {
                    text-align: center;
                    color: white;
                    z-index: 2;
                }

                .slide-content h3 {
                    font-size: 2.5rem;
                    margin-bottom: 1rem;
                    font-weight: var(--font-bold);
                }

                .carousel-btn {
                    position: absolute;
                    top: 50%;
                    transform: translateY(-50%);
                    width: 50px;
                    height: 50px;
                    background: rgba(255, 255, 255, 0.2);
                    border: none;
                    border-radius: 50%;
                    color: white;
                    font-size: 1.5rem;
                    cursor: pointer;
                    transition: background 0.2s ease;
                    z-index: 3;
                }

                .carousel-btn:hover {
                    background: rgba(255, 255, 255, 0.3);
                }

                .carousel-prev { left: 20px; }
                .carousel-next { right: 20px; }

                .carousel-indicators {
                    display: flex;
                    justify-content: center;
                    gap: 10px;
                    margin-top: 1rem;
                }

                .indicator {
                    width: 12px;
                    height: 12px;
                    border-radius: 50%;
                    border: none;
                    background: rgba(255, 255, 255, 0.3);
                    cursor: pointer;
                    transition: background 0.2s ease;
                }

                .indicator.active {
                    background: var(--primary-color);
                }

                /* Animations Demo Styles */
                .animations-demo-grid {
                    display: grid;
                    grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
                    gap: 2rem;
                    margin-bottom: 3rem;
                }

                .animation-item { display: flex; justify-content: center; align-items: center; height: auto; }
                .animation-box {
                    display: inline-flex;
                    align-items: center;
                    gap: .6rem;
                    padding: .8rem 1rem;
                    background: var(--bg-card);
                    border: 1px solid var(--border-color);
                    border-radius: 999px;
                    color: var(--text-primary);
                    font-weight: var(--font-medium);
                    box-shadow: var(--shadow-sm);
                    opacity: 0;
                    transform: translateY(16px);
                    transition: all .45s ease;
                }
                .ani-icon { display:inline-flex; width:28px; height:28px; align-items:center; justify-content:center; border-radius:50%; background: var(--gradient-primary); color: var(--text-inverse); }
                .animation-item.animate .animation-box { opacity: 1; transform: translateY(0); }

                /* Counters Demo Styles */
                .counters-demo-grid {
                    display: grid;
                    gap: 3rem;
                    margin-bottom: 2rem;
                }

                .counter-group {
                    display: grid;
                    grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
                    gap: 2rem;
                }

                .counter-item {
                    text-align: center;
                    padding: 2rem;
                    background: var(--bg-card);
                    border-radius: var(--rounded-xl);
                    border: 1px solid rgba(255, 255, 255, 0.1);
                }

                .counter-value {
                    font-size: 3rem;
                    font-weight: var(--font-bold);
                    color: var(--primary-color);
                    margin-bottom: 0.5rem;
                }

                .counter-label {
                    color: var(--text-secondary);
                }

                .progress-group {
                    display: grid;
                    gap: 1.5rem;
                }

                .progress-item {
                    padding: 1rem;
                    background: var(--bg-card);
                    border-radius: var(--rounded-lg);
                    border: 1px solid rgba(255, 255, 255, 0.1);
                }

                .progress-info {
                    display: flex;
                    justify-content: space-between;
                    margin-bottom: 0.5rem;
                    color: var(--text-secondary);
                }

                .progress-bar {
                    height: 8px;
                    background: rgba(255, 255, 255, 0.1);
                    border-radius: var(--rounded-full);
                    overflow: hidden;
                }

                .progress-fill {
                    height: 100%;
                    background: var(--gradient-primary);
                    width: 0%;
                    transition: width 2s ease;
                    border-radius: var(--rounded-full);
                }

                .circular-progress {
                    text-align: center;
                    margin-top: 2rem;
                }

                .circular-chart {
                    width: 120px;
                    height: 120px;
                    margin-bottom: 1rem;
                }

                .circle-bg {
                    fill: none;
                    stroke: rgba(255, 255, 255, 0.1);
                    stroke-width: 2;
                }

                .circle {
                    fill: none;
                    stroke: var(--primary-color);
                    stroke-width: 2;
                    stroke-linecap: round;
                    transition: stroke-dasharray 2s ease;
                }

                .percentage {
                    fill: var(--text-primary);
                    font-size: 0.5rem;
                    text-anchor: middle;
                    font-weight: var(--font-bold);
                }

                .circular-label {
                    color: var(--text-secondary);
                    font-size: 0.9rem;
                }

                .counter-controls {
                    text-align: center;
                }

                /* Masonry Demo Styles */
                .masonry-controls {
                    text-align: center;
                    margin-bottom: 3rem;
                }

                .filter-btn {
                    padding: 0.75rem 1.5rem;
                    margin: 0 0.5rem;
                    background: transparent;
                    border: 1px solid rgba(255, 255, 255, 0.3);
                    border-radius: var(--rounded-lg);
                    color: var(--text-secondary);
                    cursor: pointer;
                    transition: all 0.2s ease;
                }

                .filter-btn.active,
                .filter-btn:hover {
                    background: var(--primary-color);
                    border-color: var(--primary-color);
                    color: white;
                }

                .masonry-grid {
                    display: grid;
                    grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));
                    gap: 1rem;
                    grid-auto-rows: 10px;
                }

                .masonry-item {
                    transition: all 0.3s ease;
                }

                .masonry-item.hidden {
                    opacity: 0;
                    transform: scale(0.8);
                }

                .masonry-content {
                    border-radius: var(--rounded-lg);
                    position: relative;
                    overflow: hidden;
                    cursor: pointer;
                    transition: transform 0.2s ease;
                }

                .masonry-content:hover {
                    transform: translateY(-5px);
                }

                .masonry-overlay {
                    position: absolute;
                    bottom: 0;
                    left: 0;
                    right: 0;
                    background: linear-gradient(transparent, rgba(0, 0, 0, 0.8));
                    padding: 2rem 1.5rem 1.5rem;
                    color: white;
                    transform: translateY(100%);
                    transition: transform 0.2s ease;
                }

                .masonry-content:hover .masonry-overlay {
                    transform: translateY(0);
                }


                /* Responsive Design */
                @media (max-width: 768px) {
                    .form-row {
                        grid-template-columns: 1fr;
                    }
                    
                    .demo-form {
                        padding: 2rem;
                    }
                    
                    .mega-feature-title {
                        font-size: clamp(2rem, 6vw, 4rem);
                    }
                    
                    .counter-group {
                        grid-template-columns: 1fr;
                    }
                    
                    .masonry-grid {
                        grid-template-columns: repeat(auto-fill, minmax(200px, 1fr));
                    }
                }
            </style>

